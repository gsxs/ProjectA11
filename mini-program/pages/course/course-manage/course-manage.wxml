<!--pages/class/classes-control/classes-control.wxml-->
<view class="page">
  <view class="page__hd">
    <view class="page__title">课程管理中心</view>
    <view class="page__desc">管理课程</view>
  </view>
  <view class="page__bd">
    <view class="weui-grids">
      <block wx:for="{{apps}}" wx:key="*this">
        <navigator url="{{item.url}}" class="weui-grid" hover-class="weui-grid_active">
          <image class="weui-grid__icon" src="{{item.image}}" />
          <view class="weui-grid__label">{{item.title}}</view>
        </navigator>
      </block>
    </view>
  </view>
</view>
